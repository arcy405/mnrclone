<% content_for :title, "Bus Booking Agents-MNRYP" %>
<%= render 'Homepage/navmenu.html.erb' %>
<div class="container pt-4 widget stacked widget-table action-table">
    				
				<div class="widget-header">
					<i class="fa fa-bus" aria-hidden="true"></i>
					<h3>Bus Booking Agents</h3>
				</div> 
				
				<div class="widget-content">
					<table class="table table-striped table-bordered">
						<thead>
							<tr>
								<th>Name</th>
								<th>Address</th>
								<th>Phone</th>
							</tr>
						</thead>
						<tbody>
                        <% @agents.each do |agent|%>
							<tr>
								<td><%= agent.name %></td>
								<td><%= agent.address %></td>
								<td><%= agent.phone %></td>
							</tr>
                        <% end %>
							</tbody>
						</table>
					
				</div>
			
			</div> 
