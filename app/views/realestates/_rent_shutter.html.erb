<div class="row clearfix">
        <%= f.hidden_field :pfor, value: "shutter"  %>

    <div class="col_half">
    <div class="input_field"> 
        <%= f.select :floor, options_for_select([["Select floor", ""], "One", "Two", "Three", "Four", "Five", "Six", "Seven", "Eight"]) %>
    </div>
    </div>
    <div class="col_half">
    <div class="input_field"> 
        <%= f.select :kitchen, options_for_select([["No. of Rooms", ""], "One", "Two", "Three", "Four"]) %>
    </div>
    </div>
</div>
 <div class="input_field"> 
    </div>
    <div class="row clearfix">
    <div class="col_half">
        <div class="input_field"> 
            <%= f.text_field :length, placeholder: "Length"%>
        </div>
    </div>
    <div class="col_half">
        <div class="input_field"> 
            <%= f.text_field :breadth, placeholder: "Breadth"%>
        </div>
    </div>
</div>

<div class="text-muted"><%= t('marketplace_placeholer_5') %></div>
            <%= f.fields_for :realestate_images do |p| %>
                <div class="input-group has-validation">
                   <%= p.file_field :image, direct_upload: true,:multiple => true, class:"form-control",name: "realestate_images[image][]" %>
                   <div class="invalid-feedback">
                      Please upload property Images.
                    </div>
                </div>
                 <%= p.hidden_field :image_cache %>
           <% end %>
     <pre><%= t('marketplace_placeholer_6') %></pre>
     <hr />                   
     <%= f.submit "#{t('save')}" , class:"button form-control text-white font-weight-bold" %>
